{
    "compilerOptions": {
        "outDir": "./.tsbuild/",
        "sourceMap": false,
        "target": "es2019",
        "module": "commonjs",
        "removeComments": true,
        "noImplicitAny": true,
        "strictPropertyInitialization": true,
        "strictNullChecks": true,
        "types": ["node", "webdriverio/async"]
    },
    "include": [
        "./**/*.ts",
        "node_modules/@types/jasmine/index.d.ts"
    ]
}
